{
    "name"           : "fictizia-angular",
    "description"    : "Fictizia AngularJS",
    "homepage"       : "https://fictizia.heroku.com",
    "version"        : "0.0.0",
    "license"        : "None",
    "private"        : true,
    "authors"        : [
        "Eduardo Garc√≠a Sanz <eduardo@comakai.com>"
    ],
    "keywords"       : [
        "angular",
        "angularjs",
        "fictizia"
    ],
    "dependencies"   : {
        "angular"            : "1.4",
        "angular-ui-router"  : "0.2",
        "browserify"         : "11.0",
        "del"                : "1.2",
        "express"            : "4.13",
        "gulp"               : "3.9",
        "gulp-autoprefixer"  : "2.3",
        "gulp-ng-annotate"   : "1.1",
        "gulp-stylus"        : "2.0",
        "gulp-uglify"        : "1.2",
        "moment"             : "2.10",
        "run-sequence"       : "1.1",
        "socket.io"          : "1.3",
        "socket.io-client"   : "1.3",
        "stringify"          : "3.1",
        "twit"               : "2.1",
        "vinyl-buffer"       : "1.0",
        "vinyl-source-stream": "1.1"
    },
    "devDependencies": {
        "eslint"         : "1.2",
        "forever-monitor": "1.6",
        "gulp-livereload": "3.8",
        "gulp-notify"    : "2.2",
        "gulp-open"      : "1.0",
        "gulp-plumber"   : "1.0",
        "gulp-watch"     : "4.3",
        "watchify"       : "3.3"
    },
    "scripts"        : {
        "lint"       : "eslint src",
        "postinstall": "gulp deploy",
        "start"      : "node server"
    },
    "engines"        : {
        "iojs": "3.0"
    },
    "browserify"     : {
        "transform": [
            "stringify"
        ]
    },
    "eslintConfig"   : {
        "ecmaFeatures": {
            "modules"       : true,
            "generators"    : true,
            "arrowFunctions": true
        },
        "env"         : {
            "browser": true,
            "node"   : true,
            "es6"    : true
        },
        "rules"       : {
            "strict"                     : 0,
            "new-cap"                    : 0,
            "eqeqeq"                     : 2,
            "curly"                      : 2,
            "quotes"                     : [
                1,
                "single"
            ],
            "no-unreachable"             : 2,
            "space-before-blocks"        : 2,
            "space-before-function-paren": 2,
            "no-multi-spaces"            : 0,
            "key-spacing"                : 0,
            "no-mixed-requires"          : 0,
            "generator-star-spacing"     : [
                2,
                {
                    "before": true,
                    "after" : false
                }
            ]
        }
    }
}
